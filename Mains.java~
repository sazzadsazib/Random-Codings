import java.util.Scanner;
public class Main{
  public static void main (String[]args){
    Scanner s = new Scanner (System.in);
    while(s.hasNext()){
    String str = s.nextLine();
      for(int i = 0; i < str.length(); i++){
        char c = str.charAt(i);
        int n = (int)c;
        int p = n-7;
        System.out.print((char)p);
      }
      System.out.println();
    }
  }
}